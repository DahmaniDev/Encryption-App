<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.HomeController">
   <children>
      <TextField fx:id="toEncryptTextField" layoutX="142.0" layoutY="79.0" prefHeight="25.0" prefWidth="316.0" promptText="Enter something to encrypt" />
      <TextField fx:id="toDecryptTextField" layoutX="142.0" layoutY="224.0" prefHeight="25.0" prefWidth="316.0" promptText="Enter something to decrypt" />
      
      <Button fx:id="encryptButton" layoutX="466.0" layoutY="79.0" mnemonicParsing="false" onAction="#encryptOnClick" text="OK" />
      <Button fx:id="decryptButton" layoutX="466.0" layoutY="224.0" mnemonicParsing="false" onAction="#decryptOnClick" text="OK" />
      <TextField fx:id="encryptResult" layoutX="142.0" layoutY="125.0" prefHeight="25.0" prefWidth="316.0" style="-fx-border-color: none; -fx-background-color: none;" />
      <TextField fx:id="decryptResult" layoutX="142.0" layoutY="274.0" prefHeight="25.0" prefWidth="316.0" style="-fx-border-color: none; -fx-background-color: none;" />
      <Label layoutX="103.0" layoutY="83.0" prefHeight="17.0" prefWidth="33.0" text="Entry:" />
      <Label layoutX="103.0" layoutY="228.0" prefHeight="17.0" prefWidth="33.0" text="Entry:" />
      <Button fx:id="emptyBt" layoutX="517.0" layoutY="224.0" mnemonicParsing="false" onAction="#emptyFields" text="Empty" />
      <Label layoutX="95.0" layoutY="129.0" prefHeight="17.0" prefWidth="41.0" text="Result :" />
      <Label layoutX="95.0" layoutY="278.0" prefHeight="17.0" prefWidth="41.0" text="Result :" />
   </children>
</AnchorPane>
